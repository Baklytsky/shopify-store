{
  "name": "andrewbaklytsky",
  "version": "1.0.0",
  "description": "",
  "main": "vite.config.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --raw --kill-others --passthrough-arguments \"npm run shopify:dev\" \"npm run vite:dev\"",
    "shopify:dev": "shopify theme dev -e dev",
    "vite:dev": "vite",
    "pull:settings": "shopify theme pull --only='config/*.json'  --only='sections/*.json'  --only='templates/*.json'",
    "pull:updates": "shopify theme pull",
    "deploy": "npm run build && npm run shopify:push",
    "shopify:push": "shopify theme push -e dev",
    "build": "vite build",
    "login": "shopify theme open -s andrewbaklytsky.myshopify.com"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "postcss": "^8.4.33",
    "postcss-load-config": "^5.0.2",
    "sass": "^1.72.0",
    "vite": "^5.2.5",
    "vite-plugin-shopify": "^3.0.1"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "axios": "^1.6.8",
    "pinia": "^2.1.7",
    "vite-plugin-page-reload": "^0.2.0",
    "vue": "^3.4.21"
  }
}
